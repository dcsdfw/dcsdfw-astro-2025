          <div class="flex flex-col sm:flex-row gap-4 justify-center">
            <a href="tel:+14692600000" class="inline-flex items-center px-6 py-3 bg-brand-gold text-white rounded-lg font-semibold hover:bg-brand-gold/90 transition-colors">
              <Clock className="h-5 w-5 mr-2" />
              Call Now: (469) 260-0000
            </a>
            <a href="#contact" class="inline-flex items-center px-6 py-3 border-2 border-brand-gold text-brand-gold rounded-lg font-semibold hover:bg-brand-gold hover:text-white transition-colors">
              <MapPin className="h-5 w-5 mr-2" />
